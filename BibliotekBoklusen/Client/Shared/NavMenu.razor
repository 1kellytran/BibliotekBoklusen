
<MudNavMenu>
    <MudNavLink Href="/" Match="NavLinkMatch.All">Biblioteket Boklusen</MudNavLink>
    <AuthorizeView Roles="Librarian, Admin">
        <MudNavGroup Title="Admin" Expanded="true">
            <AuthorizeView Context="_context" Roles="Admin">
                <MudNavLink Href="/admin" Icon="@Icons.Filled.LibraryAdd" Match="NavLinkMatch.Prefix">Administrering</MudNavLink>
                     <MudNavGroup Title="Personal" Expanded="false">
                <MudNavLink Href="/createproduct" Icon="@Icons.Filled.LibraryAdd" Match="NavLinkMatch.Prefix">Administrera personal</MudNavLink>
            </MudNavGroup>
            </AuthorizeView>
            <MudNavGroup Title="Produkter" Expanded="false">
                <MudNavLink Href="/createproduct" Icon="@Icons.Filled.LibraryAdd" Match="NavLinkMatch.Prefix">Lägg till produkter</MudNavLink>
            </MudNavGroup>
            <MudNavGroup Title="Seminarium" Expanded="false">
                <MudNavLink Href="/registerseminar" Icon="@Icons.Filled.LibraryAdd" Match="NavLinkMatch.Prefix">Skapa seminarium</MudNavLink>
            </MudNavGroup>
        </MudNavGroup>
    </AuthorizeView>
    <AuthorizeView>
        <Authorized>
            <MudNavGroup Title="Profil" Expanded="false">
                <MudNavLink Href="/myprofile" Icon="@Icons.Filled.AccountBox" Match="NavLinkMatch.Prefix">Min sida</MudNavLink>
                <MudNavLink Href="/myprofile" Icon="@Icons.Filled.ArtTrack" Match="NavLinkMatch.Prefix">Mina lån</MudNavLink>
            </MudNavGroup>
            <MudNavLink Href="/logout" Icon="@Icons.Filled.Logout">Logga ut</MudNavLink>
        </Authorized>
        <NotAuthorized>
            <MudNavLink Href="/login" Icon=@Icons.Filled.Login>Logga in</MudNavLink>
            <MudNavLink Href="/register" Icon=@Icons.Filled.AppRegistration>Registrera</MudNavLink>
        </NotAuthorized>
    </AuthorizeView>
    <MudNavLink Href="/about" Match="NavLinkMatch.Prefix">About</MudNavLink>
</MudNavMenu>